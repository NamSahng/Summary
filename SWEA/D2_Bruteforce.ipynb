   "source": [
    "# 부분집합\n",
    "arr = [2,3,4,5]\n",
    "for i in range(1<<len(arr)): # 부분집합의 개수: 16개\n",
    "    print([arr[j] for j in range(len(arr)) if i & (1<<j)])"
   ]
  },
